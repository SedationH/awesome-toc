{
  "background": {
    "service_worker": "background.ts"
  },
  "content_scripts": [
    {
      "js": ["content/index.ts"],
      "css": ["content/Tree.css", "content/style.css"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "action": {
    "default_title": "Click to show XX"
  },
  "permissions": ["activeTab", "storage"],
  "version": "0.0.0",
  "manifest_version": 3,
  "options_page": "pages/options/index.html",
  "name": "awesome-toc"
}
